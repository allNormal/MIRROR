<#TriplesMap10>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_79947.\"ID\" FROM (\"person\" AS t_79947 JOIN \"professor\" AS t_21644 ON ((t_79947.\"ID\"=t_21644.\"ID\")))" 
	];

	rr:subjectMap [
