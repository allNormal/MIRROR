@prefix ex: <http://example.com> .

@base <http://mappingpedia.org/rdb2rdf/r2rml/tc/> .

<#TriplesMap1>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:tableName "patient"; 
	];

	rr:subjectMap [
		rr:template "http://example.com/patient/{ID}";
		rr:termType rr:IRI;
		rr:class <http://example.com/patient>;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#id>;
		rr:objectMap [ rr:column "ID"; rr:datatype xsd:integer; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#firstname>;
		rr:objectMap [ rr:column "FirstName"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#lastname>;
		rr:objectMap [ rr:column "LastName"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#sex>;
		rr:objectMap [ rr:column "Sex"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#weight>;
		rr:objectMap [ rr:column "Weight"; rr:datatype xsd:double; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#height>;
		rr:objectMap [ rr:column "Height"; rr:datatype xsd:double; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#birthdate>;
		rr:objectMap [ rr:column "BirthDate"; rr:datatype xsd:date; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#entrancedate>;
		rr:objectMap [ rr:column "EntranceDate"; rr:datatype xsd:dateTime; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#paidinadvance>;
		rr:objectMap [ rr:column "PaidInAdvance"; rr:datatype xsd:integer; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/patient#photo>;
		rr:objectMap [ rr:column "Photo"; rr:datatype xsd:hexBinary; ];
	].

<#TriplesMap2>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT 1 FROM INFORMATION_SCHEMA.TABLES LIMIT 1" 
	];

	rr:subjectMap [
		rr:constant <http://example.com/patient>;
		rr:termType rr:IRI;
		rr:class owl:Class;
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:subClassOf;
		rr:objectMap [ rr:constant owl:Thing ];
	].

