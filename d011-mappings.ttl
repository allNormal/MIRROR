<#TriplesMap15>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_41130.\"ID_Student\" AS ID_Student, t_41130.\"ID_Sport\" AS ID_Sport FROM (\"sport\" AS t_41044 JOIN \"student_sport\" AS t_41130 ON ((t_41044.\"ID\"=t_41130.\"ID_Sport\"))) JOIN \"student\" AS t_42337 ON ((t_42337.\"ID\"=t_41130.\"ID_Student\"))" 
	];

	rr:subjectMap [
		rr:template "http://example.com/Sport/{ID_Sport}";
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/SportHasStudent>;
		rr:objectMap [ rr:template "http://example.com/Student/{ID_Student}" ];
	].

