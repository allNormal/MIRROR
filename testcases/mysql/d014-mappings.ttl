<#TriplesMap14>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_67767.\"deptno\" FROM (\"dept\" AS t_67767 JOIN \"emp\" AS t_40435 ON ((t_67767.\"deptno\"=t_40435.\"deptno\")))" 
	];

	rr:subjectMap [
		rr:template "http://example.com/dept/{deptno}";
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/deptHasemp>;
		rr:objectMap [ rr:template "http://example.com/emp/{deptno}" ];
	].

