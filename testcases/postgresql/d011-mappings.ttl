<#TriplesMap15>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_09595.\"ID_Student\" AS ID_Student, t_09595.\"ID_Sport\" AS ID_Sport FROM (\"Sport\" AS t_68232 JOIN \"Student_Sport\" AS t_09595 ON ((t_68232.\"ID\"=t_09595.\"ID_Sport\"))) JOIN \"Student\" AS t_42279 ON ((t_42279.\"ID\"=t_09595.\"ID_Student\"))" 
	];

	rr:subjectMap [
		rr:template "http://example.com/Sport/{ID_Sport}";
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/SportHasStudent>;
		rr:objectMap [ rr:template "http://example.com/Student/{ID_Student}" ];
	].

