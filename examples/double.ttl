<#TriplesMap1>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:tableName "parent"; 
	];

	rr:subjectMap [
		rr:template "http://example.com/parent/{ID_FIRST}/parent/{ID_SECOND}";
		rr:termType rr:IRI;
		rr:class <http://example.com/Parent>;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/Parent#id_first>;
		rr:objectMap [ rr:column "ID_FIRST"; rr:datatype xsd:integer; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/Parent#id_second>;
		rr:objectMap [ rr:column "ID_SECOND"; rr:datatype xsd:integer; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/Parent#comments>;
		rr:objectMap [ rr:column "COMMENTS"; rr:datatype xsd:string; ];
	].

<#TriplesMap2>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT 1 FROM INFORMATION_SCHEMA.TABLES LIMIT 1" 
	];

	rr:subjectMap [
		rr:constant <http://example.com/Parent#id_first>;
		rr:termType rr:IRI;
		rr:class owl:DatatypeProperty;
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:domain;
		rr:objectMap [ rr:constant <http://example.com/Parent> ];
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:range;
		rr:objectMap [ rr:constant xsd:integer ];
	].

