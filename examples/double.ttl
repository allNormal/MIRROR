<#TriplesMap13>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_00660.\"ID_SECOND\", t_21992.\"ID_FIRST\" FROM (\"parent\" AS t_00660 JOIN \"child\" AS t_03135 ON ((t_21992.\"ID_FIRST\"=t_03135.\"ID_FIRST\")))" 
	];

	rr:subjectMap [
		rr:template "http://example.com/Parent/{ID_FIRST}";
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/ParentHasChild>;
		rr:objectMap [ rr:template "http://example.com/Child/{ID_FIRST}" ];
	].

