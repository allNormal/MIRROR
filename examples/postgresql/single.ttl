<#TriplesMap10>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_50143.\"ID\" FROM (\"person\" AS t_50143 JOIN \"professor\" AS t_66191 ON ((t_50143.\"ID\"=t_66191.\"ID\")))" 
	];

	rr:subjectMap [
		rr:template "http://example.com/person/{ID}";
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/personHasprofessor>;
		rr:objectMap [ rr:template "http://example.com/professor/{ID}" ];
	].

