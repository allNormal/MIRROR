	rr:predicateObjectMap [
		rr:predicate <http://example.com/student#birthdate>;
		rr:objectMap [ rr:column "\"birthdate\""; rr:datatype xsd:time; ];
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/student#ref-sport>;
		rr:objectMap [
			rr:parentTriplesMap <#TriplesMap1>;
			rr:joinCondition [
				rr:child "sport";
				rr:parent "id";
			];
		];
	].

