<#TriplesMap14>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_39207.\"deptno\" FROM (\"dept\" AS t_39207 JOIN \"emp\" AS t_29589 ON ((t_39207.\"deptno\"=t_29589.\"deptno\")))" 
	];

	rr:subjectMap [
		rr:template "http://example.com/Dept/{deptno}";
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/DeptHasEmp>;
		rr:objectMap [ rr:template "http://example.com/Emp/{deptno}" ];
	].

